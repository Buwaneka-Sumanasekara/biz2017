	<queryString>
		<![CDATA[SELECT SP.T_STOCKMST_ID,(CASE SP.UTILIZED WHEN 1 then '' WHEN 0 THEN 'UNREALIZED' END) as STATE_OF_PAY,
SP.TRNTYP,SP.PAYHEDID,SP.PAYDETID,SUM(SP.AMOUNT)AS AMT,sp.EFT_DATE
FROM T_STOCKPAYMENTS SP INNER JOIN U_TRANSACTIONS T ON SP.TRNTYP=T.TRNTYPE

WHERE SP.TRNTYP='INV' AND SP.EFT_DATE BETWEEN $P{PARA_FDATE} AND $P{PARA_EDATE}

GROUP BY  SP.T_STOCKMST_ID,SP.UTILIZED,SP.TRNTYP,SP.PAYHEDID,SP.PAYDETID,sp.EFT_DATE]]>
	</queryString>
